		"@types/peerjs": "^1.1.0",
		"canvas-screenshot": "^3.0.0",
		"dexie": "^3.0.3",
		"jszip": "^3.5.0",
		"mesh-particle-system": "^0.9.0",
		"mobile-device-detect": "^0.4.3",
